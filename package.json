{
  "name": "amarumed",
  "version": "0.0.1",
  "description": "amaruMed es una api para la gestión de procesos hospitalarios.",
  "author": "Andres Grijalva",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/tests\"",
    "test:dev": "lb-mocha --allow-console-logs dist/tests/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrations/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "build:client": "cd client && npm run build",
    "serve:client": "cd client && npm run serve",
    "serve:api": "npm run clean && npm run build && node -r source-map-support/register .",
    "serve": "npm run serve:api & npm run serve:client",
    "prestart": "npm run clean && npm run build && npm run build:client",
    "preinstall": "cd client && npm install",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "dependencies": {
    "@loopback/authentication": "^7.0.0",
    "@loopback/boot": "^3.0.0",
    "@loopback/core": "^6.1.11",
    "@loopback/repository": "^3.0.0",
    "@loopback/rest": "^14.0.14",
    "@loopback/rest-explorer": "^3.0.0",
    "@loopback/service-proxy": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-postgresql": "^5.0.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.12",
    "@loopback/eslint-config": "^10.0.0",
    "@loopback/testlab": "^3.2.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.0.4",
    "@types/express-serve-static-core": "4.17.18",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^10.17.34",
    "@types/nodemailer": "^6.4.0",
    "eslint": "^7.9.0",
    "prettier": "^2.1.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1"
  },
  "engines": {
    "node": ">=10.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paquisha"
  },
  "keywords": [
    "amarumed",
    "hospital",
    "gestión"
  ],
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/tests"
  ],
  "copyright.owner": "Andres Grijalva"
}
